  "f7-http-request-post": {
    "description": "",
    "scope": "",
    "prefix": "f7-http-request-post",
    "body": [
      "function post() {",
      "\tvar data_post = {",
      "\t\tattribute_1: \"value_1\",",
      "\t\tattribute_2: \"value_2\"",
      "\t}",
      "\tvar request = Framework7.request({",
      "\t\tasync: false,",
      "\t\tcrossDomain: true,",
      "\t\tmethod: \"POST\",",
      "\t\turl: \"https://myapi/post\",",
      "\t\tdata: data_post,",
      "\t\tsuccess: function(data) {",
      "\t\t\treturn data;",
      "\t\t}",
      "\t});",
      "\tvar response = JSON.parse(request.responseText);",
      "\treturn response;",
      "}",
      "var return_post = post()"
    ]
  },
  "f7-http-request-get": {
    "description": "",
    "scope": "",
    "prefix": "f7-http-request-get",
    "body": [
      "function get(){",
      "\tvar uri = \"select_id\"",
      "\tvar request = Framework7.request({",
      "\t\tasync: false,",
      "\t\tcrossDomain: true,",
      "\t\tmethod: \"GET\",",
      "\t\turl: \"https://myapi/\", uri,",
      "\t\tsuccess: function (data) {",
      "\t\t\treturn data;",
      "\t\t}",
      "\t});",
      "\tvar response = JSON.parse(request.responseText);",
      "\treturn response;",
      "}",
      "var return_get = get();"
    ]
  },
