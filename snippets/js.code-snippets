    "description": "",
    "scope": "",
    "prefix": "f7-http-request-put",
    "body": [
      "function update() {",
      "\tvar data_post = {",
      "\t\tattribute_1: \"value_1\",",
      "\t\tattribute_2: \"value_2\"",
      "\t}",
      "\tvar request = Framework7.request({",
      "\t\tasync: false,",
      "\t\tcrossDomain: true,",
      "\t\tmethod: \"PUT\",",
      "\t\turl: \"https://myapi/update\",",
      "\t\tdata: data_post,",
      "\t\tsuccess: function(data) {",
      "\t\t\treturn data;",
      "\t\t}",
      "\t});",
      "\tvar response = JSON.parse(request.responseText);",
      "\treturn response;",
      "}",
      "var return_update = update();"
    ]
