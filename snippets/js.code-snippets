  "f7-dialog-progress-percent": {
    "description": "",
    "scope": "",
    "prefix": "f7-dialog-progress-percent",
    "body": [
      "var progress = 0;",
      "var dialog = app.dialog.progress('Loading assets', progress);",
      "dialog.setText('Image 1 of 10');",
      "var interval = setInterval(function() {",
      "\tprogress += 10;",
      "\tdialog.setProgress(progress);",
      "\tdialog.setText('Image ' + ((progress) / 10) + ' of 10');",
      "\tif (progress === 100) {",
      "\t\tclearInterval(interval);",
      "\t\tdialog.close();",
      "\t}",
      "}, 300);"
    ]
